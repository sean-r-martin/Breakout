{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "check-node-version --node '>= 6.7.0' && nodemon server/start.js",
    "build": "check-node-version --node '>= 6.7.0' && webpack",
    "build-watch": "check-node-version --node '>= 6.7.0' && webpack -w",
    "build-css": "node-sass public/stylesheets/style.scss -o public/stylesheets",
    "build-watch-css": "node-sass -w public/stylesheets/style.scss -o public/stylesheets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sean-r-martin/react-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sean-r-martin/react-boilerplate/issues"
  },
  "homepage": "https://github.com/sean-r-martin/react-boilerplate#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "check-node-version": "^2.0.1",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.30.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "node-sass": "^4.5.2",
    "webpack": "^2.3.2"
  }
}
