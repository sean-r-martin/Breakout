{
  "name": "brick-buster",
  "version": "1.0.0",
  "description": "web application game based on the classic game breakout",
  "main": "index.js",
  "scripts": {
    "test": "check-node-version --node '>= 6.7.0' && mocha --compilers js:babel-register app/**/*.test.js server/**/*.test.js",
    "test-watch": "check-node-version --node '>= 6.7.0' &&  mocha --compilers js:babel-register --watch app/**/*.test.js server/**/*.test.js",
    "start": "check-node-version --node '>= 6.7.0' && webpack-dev-server -d --hot --config webpack.config.js --watch",
    "build": "check-node-version --node '>= 6.7.0' && webpack -w",
    "start-ver2": "check-node-version --node '>= 6.7.0' && nodemon server/start.js",
    "production": "NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sean-r-martin/react-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sean-r-martin/react-boilerplate/issues"
  },
  "homepage": "https://github.com/sean-r-martin/react-boilerplate#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "check-node-version": "^3.1.1",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.13.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "image-webpack-loader": "^3.4.2",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
